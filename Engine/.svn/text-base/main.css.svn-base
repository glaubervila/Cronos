body {
    font: normal 12px verdana,arial,tahoma;
}
html, body {
    margin:0;
    padding:0;
    border:0 none;
    overflow:hidden;
    height:100%;
    width:100%;
}

#docs .top-toolbar{
    border:0 none;
    border-top:1px solid #d0d0d0;
    background:transparent;
}
#docs .top-toolbar button {

}
.x-panel-noborder {
    border: 0 none;
}
#header{
   border:0 none;
   background:#1E4176 url(Resources/hd-bg.gif) repeat-x 0 0;
   padding-top:3px;
   padding-left:3px;
   height: 40px;
   visibility: hidden;
}
.docs-header .x-panel-body {
    background:transparent;
}

#header {

}

#header .logotipo_banner {
    float:left;
    margin-right:10px;
    width:60px;
    height:30px;
    margin-top:0px;
	margin-left:10px;
	background:#CCCCCC;
}

#header .api-title {
    font:normal 16px tahoma, arial, sans-serif;
    color:white;
    margin:5px;
}

#header .info_usuario {
    float:right;
    margin-right:10px;
    margin-top:-12px;
    --width:83px;
    height:24px;
    color:white;
}

#header .logout {
    float:right;
    margin-right:10px;
    width:16px;
    height:16px;
    margin-top:-12px;
}

#loading-mask{
    position:absolute;
    left:0;
    top:0;
    width:100%;
    height:100%;
    z-index:20000;
    background-color:white;
}
#loading{
    position:absolute;
    top:50%;
    left:50%;
    width:300px;
    height:50px;
    margin-left:-150px;
    margin-top:-25px;
    z-index:20001;
    height:auto;
}
#loading a {
    color:#225588;
}
#loading .loading-indicator{
    background:white;
    color:#444;
    font:bold 13px tahoma,arial,helvetica;
    padding:10px;
    margin:0;
    height:auto;
}
#loading-msg {
    font: normal 10px arial,tahoma,sans-serif;
    color:red;
}
#ext-msg {
    font: normal 10px arial,tahoma,sans-serif;
}

/* Server Status */
#div_uptime_value {}
.uptime_label{
    --padding-left:20px;
    font:bold 10px tahoma,arial,helvetica;
    color:#444;
}
.uptime_value{
    font:bold 10px tahoma,arial,helvetica;
    color:#427FB0;
}


/* SERVIDOR OFFLINE */
#servidor_offline {
    width:100%;
    height:100%;
    padding-top:50px;
    font:bold 22px tahoma,arial,helvetica;
    text-align:center;
}

/* Icones */

/* Integracao Emporium */
.silk-emporium { background-image: url(Resources/emporium.png) !important; background-repeat: no-repeat; }

.silk-novo-gif { background-image: url(Resources/novo.gif) !important; background-repeat: no-repeat; width:36px;}

.silk-visualizar { background-image: url(Resources/page_edit.gif) !important; background-repeat: no-repeat; width:36px;}

